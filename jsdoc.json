{
    "plugins": ["jsdoc-http-plugin"],
    "source": {
        "include": [ "src" ],
        "exclude": [ "out", "node_modules" ],
        "includePattern": ".js$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./documentation"
    }
}